{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "target": "es5",
    "jsx": "react",
    "declaration": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "paths": {
      "*": ["../../ladingTypes/*"]
    }
  }
}
